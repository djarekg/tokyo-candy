{
  "name": "@tc/db",
  "private": false,
  "version": "1.0.0",
  "packageManager": "bun@1.3.3",
  "engines": {
    "bun": "1.3.3"
  },
  "type": "module",
  "imports": {
    "#app/*": "./src/*",
    "#app/generated/prisma/client/*": "./src/generated/prisma/client/*",
    "#prisma/seed/*": "./prisma/seed/*"
  },
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "import": "./src/client/index.ts"
    },
    "./constants": {
      "import": "./src/constants/index.ts"
    },
    "./enums": {
      "import": "./src/generated/prisma/enums/*"
    },
    "./models": {
      "import": "./src/generated/prisma/models/*"
    },
    ".": {
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "db:clean": "bunx prisma migrate reset --force",
    "db:migrate": "bunx prisma migrate dev",
    "db:generate": "bunx prisma generate",
    "db:seed": "bun prisma/seed/seed.ts",
    "db": "bun run db:clean && bun run db:generate && bun run db:migrate && bun run db:seed"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@prisma/extension-accelerate": "2.0.2",
    "bcryptjs": "3.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@prisma/adapter-pg": "^6.18.0",
    "@types/bun": "1.3.2",
    "prisma": "^6.18.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "trustedDependencies": [
    "@prisma/client",
    "@prisma/engines",
    "prisma"
  ]
}
