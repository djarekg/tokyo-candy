{
  "name": "@tc/components",
  "private": false,
  "version": "1.0.0",
  "packageManager": "bun@1.3.3",
  "engines": {
    "bun": "1.3.3"
  },
  "type": "module",
  "imports": {
    "#app/*": "./src/*"
  },
  "exports": {
    "./package.json": "./package.json",
    "./command-palette": {
      "import": "./src/command-palette/command-palette.tsx"
    },
    "./command-palette/index": {
      "import": "./src/command-palette/index.ts"
    },
    "./form": {
      "import": "./src/form/index.tsx"
    },
    "./icons/*": {
      "import": "./src/icons/*.tsx"
    },
    "./loader": {
      "import": "./src/loader/loader.tsx"
    },
    "./styles/index.css": {
      "import": "./src/styles/index.css"
    },
    ".": {
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsgo"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.72.7",
    "@fluentui/react-icons": "2.0.315",
    "@tc/core": "workspace:*",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/bun": "1.3.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
